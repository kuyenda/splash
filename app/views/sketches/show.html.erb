<% provide(:title, @sketch.title) %>
<div class="render">
  <div class="render-nav">
    <%= render partial: 'sketches/render/navbar' %>
  </div>
  <div class="render-tab-wrapper">
    <div class="render-tab" id="copyrightTab">
      <%= render partial: 'sketches/render/cover' %>
    </div>
    <div class="render-tab active" id="frameTab">
      <%= render partial: 'sketches/render/frame' %>
    </div>
    <div class="render-tab" id="editorTab">
      <%= render partial: 'sketches/render/editor' %>
      <%= render partial: 'sketches/render/config' %>
    </div>
  </div>
</div>
<script>
  $(function() {window.app.init();});
</script>