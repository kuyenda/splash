<div class="d116">
  <div class="container">
    <div>
      <h1>Examples#new</h1>
      <p>Find me in app/views/examples/new.html.erb</p>
    </div>
    <div class="row bg-transparent">
      <div class="col py-md-4">
        <%= form_for(@example) do |f| %>
          <h1 class="text-center">Sketch.new</h1>
          <%= f.text_field :title, placeholder: "Title" %>
          <%= f.text_field :description, placeholder: "Description" %>
          <%= f.hidden_field :hit, value: 0 %>
          <%= f.hidden_field :like, value: 0 %>
          <%= render 'shared/error_messages' %>
          <%= f.submit "Create" %>
          <div class="form-tool">
            <%= link_to "返回", examples_path %>
          </div>
        <% end %>
      </div>
    </div>
  </div>
</div>