<h4 class="x header block">
  <%= bootstrap_icon "geo" %>
  <%= FFaker::AddressJA.county %>
  <div class="x header lead"><%= FFaker::AddressJA.designated_city %></div>
</h4>
<!--  -->
<h3 class="x header block">大小写、水平对齐
  <p class="x header lead">text-align text-transform</p>
</h3>
<div class="row row-cols-1">
  <div class="col x text left capitalize bggray0">
    <span>x text left capitalize</span>
  </div>
  <div class="col x text center lowercase bggray0">
    <span>x text center lowercase</span>
  </div>
  <div class="col x text right uppercase bggray0">
    <span>x text right uppercase</span>
  </div>
</div>
<!--  -->
<h3 class="x header block">文本装饰
  <p class="x header lead">text-decoration</p>
</h3>
<div class="row row-cols-1">
  <div class="col x text strike">
    <p>x text strike</p>
  </div>
  <div class="col x text underline">
    <p>x text underline</p>
  </div>
  <div class="col x text no-underline">
    <p>x text no-underline</p>
  </div>
</div>
<!--  -->
<h3 class="x header block">垂直对齐
  <p class="x header lead">text-decoration</p>
</h3>
<table class="table" style="height: 100px;">
  <tbody>
    <tr>
      <td class="x text baseline">x text baseline</td>
      <td class="x text top">x text top</td>
      <td class="x text middle">x text middle</td>
      <td class="x text bottom">x text bottom</td>
    </tr>
  </tbody>
</table>
<!--  -->
<h3 class="x header block table-bordered">字号
  <p class="x header lead">font-size</p>
</h3>
<table class="table table-responsive">
  <tbody>
    <tr>
      <% for i in 1..6 %>
        <td class="x font size<%= i %>">x font size<%= i %></td>
      <% end %>
    </tr>
  </tbody>
</table>
<h3 class="x header block table-bordered">字重
  <p class="x header lead">font-weight</p>
</h3>
<table class="table table-responsive">
  <tbody>
    <tr>
      <% for i in 1..9 %>
        <td class="x font weight<%= i %>">x font weight<%= i %></td>
      <% end %>
    </tr>
  </tbody>
</table>
<h3 class="x header block table-bordered">字体
  <p class="x header lead">font-family</p>
</h3>
<% {'EN':"Steradian",
'EN2':"ProximaSans",
'CN':"Xianghe",
'MONO':"menlo",
'SERIF':"Noto Serif",
'TITLE':"Ramona"}.each do |k, v| %>
  <p class="x font <%= k %> weight5 size5">x font <%= k %>: Lorem ipsum dolor sit amet, consectetur adipisicing elit. Ea consequatur rerum eligendi id totam ex laboriosam laborum reiciendis, alias autem dolor amet voluptates nostrum optio, dolores tenetur necessitatibus. Mollitia, totam!</p>
<% end %>